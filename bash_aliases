# Naviation Helpers
alias ~='cd ~'
alias .='cd ..'
alias ..='cd ../..'
alias ...='cd ../../..'
alias ....='cd ../../../..'

# Git Helpers
alias st='git status'
alias gd='git diff'

# Directory Listing Helpers
alias la='ls -al'

# Others
alias vim='vim -u ~/.vim/.vimrc'	# Source vimrc from .vim directory which you can now check in
