# Naviation Helpers
alias ~='cd ~'
alias .='cd ..'
alias ..='cd ../..'
alias ...='cd ../../..'
alias ....='cd ../../../..'

# Git Helpers
alias st='git status'
alias gd='git diff'

# Directory Listing Helpers
alias la='ls -al'

alias ssh-key-make='ssh-keygen -t rsa -b 2048 -C'
alias scribble='/Users/kbrockman/code/personal/scribble/scribble'

alias pip-3.6=pip3
