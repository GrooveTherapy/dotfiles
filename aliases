# echo '.aliases loaded'

# Naviation Helpers
alias ~='cd ~'
alias .='cd ..'
alias ..='cd ../..'
alias ...='cd ../../..'
alias ....='cd ../../../..'

# Git Helpers
alias st='git status'
alias gd='git diff'

# Directory Listing Helpers
alias la='ls -al'
alias lah='ls -alh'

# disk space
alias diskusage='du -h -d 1 .'

alias ssh-key-make='ssh-keygen -t rsa -b 2048 -C'

alias pip3.6=pip3
alias python3=python3.6
